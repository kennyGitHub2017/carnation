package com.sinco.carnation.shop.model;

import com.sinco.mybatis.dal.model.BaseModel;
import java.util.Date;

public class Complaint extends BaseModel {
	/**
	 * 
	 */
	private static final long serialVersionUID = -5734315384985417126L;

	/**  **/
	private Long id;

	/**  **/
	private Date addTime;

	/**  **/
	private Boolean deleteStatus;

	/** 申诉时间 **/
	private Date appealTime;

	/** 仲裁时间 **/
	private Date handleTime;

	/** 投诉状态，0为新投诉，1为待申诉，2为沟通中，3为待仲裁，4投诉结束 **/
	private Integer status;

	/** buyer为买家投诉，seller为卖家投诉 **/
	private String type;

	/** 投诉主题 **/
	private Long csId;

	/** 投诉人取证图片1 **/
	private String fromAcc1;

	/** 投诉人取证图片2 **/
	private String fromAcc2;

	/** 投诉人取证图片3 **/
	private String fromAcc3;

	/** 投诉者 **/
	private Long fromUserId;

	/** 仲裁管理员 **/
	private Long handleUserId;

	/** 投诉对应的订单 **/
	private Long ofId;

	/** 被投诉人申诉图片1 **/
	private String toAcc1;

	/** 被投诉人申诉图片1 **/
	private String toAcc2;

	/** 被投诉人申诉图片1 **/
	private String toAcc3;

	/** 被投诉 **/
	private Long toUserId;

	/** 投诉人意见 **/
	private String fromUserContent;

	/** 仲裁意见 **/
	private String handleContent;

	/** 对话记录，使用分行管理 **/
	private String talkContent;

	/** 被投诉人意见 **/
	private String toUserContent;

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public Date getAddTime() {
		return addTime;
	}

	public void setAddTime(Date addTime) {
		this.addTime = addTime;
	}

	public Boolean getDeleteStatus() {
		return deleteStatus;
	}

	public void setDeleteStatus(Boolean deleteStatus) {
		this.deleteStatus = deleteStatus;
	}

	public Date getAppealTime() {
		return appealTime;
	}

	public void setAppealTime(Date appealTime) {
		this.appealTime = appealTime;
	}

	public Date getHandleTime() {
		return handleTime;
	}

	public void setHandleTime(Date handleTime) {
		this.handleTime = handleTime;
	}

	public Integer getStatus() {
		return status;
	}

	public void setStatus(Integer status) {
		this.status = status;
	}

	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}

	public Long getCsId() {
		return csId;
	}

	public void setCsId(Long csId) {
		this.csId = csId;
	}

	public String getFromAcc1() {
		return fromAcc1;
	}

	public void setFromAcc1(String fromAcc1) {
		this.fromAcc1 = fromAcc1;
	}

	public String getFromAcc2() {
		return fromAcc2;
	}

	public void setFromAcc2(String fromAcc2) {
		this.fromAcc2 = fromAcc2;
	}

	public String getFromAcc3() {
		return fromAcc3;
	}

	public void setFromAcc3(String fromAcc3) {
		this.fromAcc3 = fromAcc3;
	}

	public Long getFromUserId() {
		return fromUserId;
	}

	public void setFromUserId(Long fromUserId) {
		this.fromUserId = fromUserId;
	}

	public Long getHandleUserId() {
		return handleUserId;
	}

	public void setHandleUserId(Long handleUserId) {
		this.handleUserId = handleUserId;
	}

