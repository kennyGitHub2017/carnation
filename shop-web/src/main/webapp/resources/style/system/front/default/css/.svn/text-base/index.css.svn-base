/*品牌*/
.index_brand_right{ width:288px; float:left; margin-top:10px;}
.brand_bottom_adv{ width:288px; float:left; margin-top:5px; height:120px; overflow:hidden;}
.brand {width:288px;float:left;}
.brand h3 {color:#333;height:37px;line-height:37px;font-size:14px;border-bottom:2px solid #eaeaea;}
.brand h3 span {float:right;background:url(../images/more.jpg) no-repeat left center;color:#555; padding-left:15px;}
.brand h3 span a {color:#999999;font-size:12px;font-weight:normal;padding-right:5px;}
.brand h3 span a:hover {color:#F00}
.brand ul {width:288px;overflow:hidden;*padding-bottom:1px;margin-top:8px; height:138px; overflow:hidden}
.brand ul li {width:142px;height:66px;float:left; border:1px solid #f5f5f5; overflow:hidden; position:relative;}
.brand ul li img {width:142px;height:66px;}
.brand ul li a{ display:block; width:142px;height:66px;position:absolute; top:0px; left:0px; text-align:center; line-height:66px;filter:alpha(opacity=0);-moz-opacity:0.0;opacity:0.0;  }
.brand ul li a:hover{ display:block;background:#40b0e0; color:#FFFFFF;filter:alpha(opacity=80);-moz-opacity:0.80;opacity:0.80; }

/*楼层样式开始------------------------------------------*/

.storey { width:100%; height:auto; overflow:hidden; margin-top:30px; float:left; border-bottom:1px solid #ccc;}
.storey_left { width:200px; float:left;}
.storey_right { width:1000px; float:right;}
.storey .hot_sell { border-top: 2px #ccc solid;}

.storey .index_sale_tab { padding-top: 2px; border-bottom-width: 3px;}
.storey .index_sale_tab .this a { color: #fff;}

/***蓝色楼层相关样式***/
.blue_storey .storey_left h1 { background-color: #19bf96;}
.blue_storey .index_sale_tab { border-bottom-color: #19bf96;}
.blue_storey .hot_sell {border-top-color: #19bf96;}
.blue_storey .index_sale_tab a,.blue_storey .goods_price { color: #19bf96;}
.blue_storey .index_sale_tab .this a { background-color: #19bf96;}
.blue_storey .index_sale_tab ul li {border-right-color: #19bf96;}


/***绿色楼层相关样式***/
.green_storey .storey_left h1 { background-color: #5faf1c;}
.green_storey .index_sale_tab { border-bottom-color: #5faf1c;}
.green_storey .hot_sell {border-top-color: #5faf1c;}
.green_storey .index_sale_tab a,.green_storey .goods_price { color: #5faf1c;}
.green_storey .index_sale_tab ul li {border-right-color: #5faf1c;}
.green_storey .index_sale_tab .this a { background-color: #5faf1c;}

/***橘色楼层相关样式***/
.orange_storey .storey_left h1 { background-color: #ff8518;}
.orange_storey .index_sale_tab { border-bottom-color: #ff8518;}
.orange_storey .hot_sell {border-top-color: #ff8518;}
.orange_storey .index_sale_tab a,.orange_storey .goods_price { color: #ff8518;}
.orange_storey .index_sale_tab ul li {border-right-color: #ff8518;}
.orange_storey .index_sale_tab .this a { background-color: #ff8518;}

/***灰色楼层相关样式***/
.gray_storey .storey_left h1 { background-color: #595959;}
.gray_storey .index_sale_tab { border-bottom-color: #595959;}
.gray_storey .hot_sell {border-top-color: #595959;}
.gray_storey .index_sale_tab a,.gray_storey .goods_price { color: #595959;}
.gray_storey .index_sale_tab ul li {border-right-color: #595959;}
.gray_storey .index_sale_tab .this a { background-color: #595959;}

/***粉色楼层相关样式***/
.pink_storey .storey_left h1 { background-color: #ff4e88;}
.pink_storey .index_sale_tab { border-bottom-color: #ff4e88;}
.pink_storey .hot_sell {border-top-color: #ff4e88;}
.pink_storey .index_sale_tab a,.pink_storey .goods_price { color: #ff4e88;}
.pink_storey .index_sale_tab ul li {border-right-color: #ff4e88;}
.pink_storey .index_sale_tab .this a { background-color: #ff4e88;}

/***蓝色1楼层相关样式***/
.blue1_storey .storey_left h1 { background-color: #059bf2;}
.blue1_storey .index_sale_tab { border-bottom-color: #059bf2;}
.blue1_storey .hot_sell {border-top-color: #059bf2;}
.blue1_storey .index_sale_tab a,.blue1_storey .goods_price { color: #059bf2;}
.blue1_storey .index_sale_tab ul li {border-right-color: #059bf2;}
.blue1_storey .index_sale_tab .this a { background-color: #059bf2;}





/***左侧广告及分类***/
.storey_left h1 { width:100%; height:43px; line-height:40px; color:#FFF; font-size:20px; font-family:"微软雅黑"; font-weight:normal; }
.storey_left h1 b { width:38px; height:35px;float:left; color:#fff; margin-left:40px; text-align:center; display:block; margin-right:10px; font-size:26px; }
.advert { width:199px; height:180px; float:left;}
.advert img { width:199px; height:180px;}
.storey_bd { width:199px; float:left;border-right:1px solid #cccccc;}
.storey_ul { width:199px; height:290px; padding-top:10px; float:left; overflow:hidden;}
.storey_ul ul li { width:89px; float:left; display:block; height:29px; line-height:30px; font-size:14px; padding-left:10px;}
.storey_ul ul li a { width:89px; float:left; white-space:nowrap;}
.storey_ul ul li a:hover { text-decoration:underline;}

/***tab***/
.storey_tab { width:100%; float:left; border-bottom:2px solid #eaeaea; height:41px;}
.storey_tab ul li { width:25%; display:block; float:left; height:41px; line-height:41px; text-align:center; font-size:15px; position:relative;*width:24%;}
.storey_tab ul li.this { height:41px; top:0px;}
.storey_tab ul li.this s {position:absolute;  line-height:0px; height:0px; border-width:7px; border-style:solid; width:0px; top:28px; left:50%; margin-left:-2px; border-style:dashed dashed solid;}
.storey_list { width:100%; height:334px; float:left;}
.storey_list ul li { width:25%; height:334px; float:left; display:block; font-size:14px; overflow:hidden}

.bd_right {height:334px; border-right:1px solid #CCC; position:relative; overflow:hidden;}
.storey_ps {overflow:hidden; padding-left:15px; cursor:pointer; padding-right:20px;  }
.storey_hover{ -moz-transition: all 0.3s ease 0s;right:-10px;position:relative;}
.storey_ps:hover .storey_hover{ right:0px; }

.goods_name,.goods_describe,.goods_price { width:100%; height:26px; line-height:26px; display:block; float:left; overflow:hidden;}
.goods_name {padding-top:6px; height:52px;}
.goods_describe { color:#F60; font-size:12px;}
.goods_price { padding:10px 0px; color:#cc0000;}
.goods_price b { font-size:24px;}
.goods_price em { font-style:normal; text-decoration:line-through; color:#999 !important; margin-left:10px;}
.goods_pic{ width:220px; height:220px; overflow:hidden}
.goods_pic span{width:220px; height:220px; overflow:hidden}

/***热销排行***/
