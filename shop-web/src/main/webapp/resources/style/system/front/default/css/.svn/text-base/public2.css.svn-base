/*购物车样式调整*/
.buy_step{margin-top: 50px;}
.buy_step ul{height: 40px;overflow: hidden;}
.buy_step ul li{float: left;width: 33.3%;height: 8px;margin-top: 16px;background: #9de6d4;position: relative;}
.buy_step ul li.curr{background: #19BF96;}
.buy_step ul li span{width: 40px;height: 40px;position: absolute;left: 178px;top: -16px;background-image:url(../images/step_num.png);background-repeat: no-repeat;border-radius: 50%;}

.buy_step ul li .s1{background-position: left bottom;}
.buy_step ul li.curr .s1{background-position: left -1px;}
.buy_step ul li .s2{background-position: -40px bottom;}
.buy_step ul li.curr .s2{background-position: -40px top;}
.buy_step ul li .s3{background-position: -80px -41px;}
.buy_step ul li.curr .s3{background-position: -80px -1px;}

.step_text{color:#85dbc6;margin-top: 15px;font-size: 0;}
.step_text span{display: inline-block;font-size:16px;width: 33.2%;text-align: center;}
.step_text span.curr{color: #19bf96;}




