}
.pointer{color: gray;font-size: 28px;position: relative;top: -2px;
	-webkit-animation: twinkling 1s steps(1,start) infinite;
	animation: twinkling 1s steps(1,start) infinite; 
}
@-webkit-keyframes twinkling{
	0%{opacity: 0}
	50%{opacity: 0}
	100%{opacity: 1}
}
