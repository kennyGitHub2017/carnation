<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<title>商家后台-退货列表- $!config.title - $!{config.poweredby}</title>
<meta name="keywords" content="$!config.keywords" />
<meta name="description" content="$!config.description" />
<meta name="generator" content="$!{config.meta_generator}" />
<meta name="author" content="$!{config.meta_author}">
<meta name="copyright" content="$!{config.copyRight}">
<link href="$!webPath/resources/style/system/front/default/css/public2.css" type="text/css" rel="stylesheet" />
<link href="$!webPath/resources/style/system/front/default/css/user.css" type="text/css" rel="stylesheet" />
<link href="$!webPath/resources/style/common/css/overlay_blue.css" type="text/css" rel="stylesheet" />
<script src="$!webPath/resources/js/jquery-1.6.2.js"></script>
<script src="$!webPath/resources/js/jquery.shop.common.js"></script>
</head>
<body>
$!httpInclude.include("/seller/top.htm")
<div class="main">
  <div class="seller_center">
    <div class="seller_center_left"> $!httpInclude.include("/seller/nav.htm?op=return") </div>
    <div class="seller_right">
      <div class="seller_position"><span><b><a href="$!webPath/seller/index.htm">商家中心</a></b>><b>客户服务</b>><b>退货管理</b></span></div>
      <div class="seller_list">
        <div class="seller_list_title">
          <ul class="seller_title_ul">
            <li class="this"><a href="$!webPath/seller/return.htm">退货列表</a></li>
          </ul>
        </div>
        #if($objs.size()>0)
        <div  class="user_sp_right">
          <form action="$!webPath/seller/return.htm" method="post">
            <div  class="seller_title_sp"> <span>商品名称</span> <span class="size150">
              <input name="goodsName" type="text" id="goodsName" value="$!vo.goodsName" />
              </span> <span>服务单号</span> <span class="size150">
              <input name="returnServiceId" type="text" id="returnServiceId" value="$!vo.returnServiceId" />
              </span> <span>申请者</span> <span>
              <input name="userName" type="text" id="userName" value="$!vo.userName" />
              </span> <span>
              <input name="" type="submit"  value="搜索" style="cursor:pointer;"  class="seller_sear_btn"/>
              </span> </div>
          </form>
        </div>
        <div class="seller_list_box">
          <form method="post" name="ListForm" id="ListForm" action="$!webPath/seller/return.htm">
            <div class="operation">
              <table width="100%" border="0" cellspacing="0" cellpadding="0" id="opertable" class="user_order_table">
                <tr id="opertitle">
                  <th width="10%">服务单号</th>
                  <th width="10%">商品图片</th>
                  <th width="10%">商品价格</th>
                  <th width="10%">商品数量</th>
                  <th width="10%">商品总价</th>
                  <th width="8%">申请者</th>
                  <th width="20%">退单状态</th>
                  <th align="center">操作</th>
                </tr>
                #foreach($obj in $objs)
                #set($goods_url="$!webPath/goods_$!{obj.goodsId}.htm")
                #if($!config.second_domain_open)
                #set($goods_url="http://$!{store.store_second_domain}.$!{domainPath}/goods_$!{obj.goodsId}.htm")
                #end
                <tr >
                  <td align="center">$!obj.returnServiceId</td>
                  <td align="center">#set($img="$!{obj.goodsMainphotoPath}") <a href="$!goods_url" target="_blank"><img src="$!img" width="70" height="70" /></a></td>
                  <td align="center">$!obj.goodsPrice</td>
                  <td align="center">$!obj.goodsCount</td>
                  <td align="center">$!obj.goodsAllPrice</td>
                  <td align="center">$!obj.userName</td>
                  #if($!obj.goodsReturnStatus==5)
                   #set($status="买家申请退货")
                  #end
                  #if($!obj.goodsReturnStatus==-1)
                   #set($status="不可退货")
                  #end
                   #if($!obj.goodsReturnStatus==6)
                   #set($status="审核通过，请等待买家退货")
                  #end
                   #if($!obj.goodsReturnStatus==7)
                   #set($status="买家已退货")
                  #end
                   #if($!obj.goodsReturnStatus==10)
                   #set($status="退货完成,等待平台退款")
                  #end
                  #if($!obj.goodsReturnStatus==11)
                   #set($status="平台已退款")
                  #end
                  <td align="center">$!{status}</td> 
                  <td align="center" class="del_operate"> #if($!obj.goodsReturnStatus==5) <a href="$!webPath/seller/return_check.htm?id=$!obj.id&currentPage=$!currentPage" class="del_a">审核</a> #end
                    #if($!obj.goodsReturnStatus==-1) <a href="javascript:void(0);">无操作</a> #end
                    #if($!obj.goodsReturnStatus==6) <a href="javascript:void(0);">无操作</a> #end
                    #if($!obj.goodsReturnStatus==7)
                   
					<p><a href="$!webPath/seller/return_check.htm?id=$!obj.id&currentPage=$!currentPage" class="del_a">查看物流</a></p>
					<p><a href="javascript:void(0);" onclick="showDialog('msg_info','','是否确认收货，确认后状态不可改变?',1,'question',3,function(){window.location.href='$!webPath/seller/return_confirm.htm?id=$!obj.id&currentPage=$!currentPage'});"  class="del_a">确认收货</a></p>

                    #end
                    #if($!obj.goodsReturnStatus==10) <a href="javascript:void(0);">无操作</a>#end </td>
                </tr>
                #end
              </table>
            </div>
            <input id="currentPage" name="currentPage" type="hidden" value="$!currentPage"/>
            <div class="seller_list_page">
              <div  class="seller_page_sp"> $!gotoPageFormHTML</div>
            </div>
          </form>
        </div>
        #else
        <table>
          <tr>
            <td colspan="8" align="center"><div class="seller_center_nothing">
                <div class="seller_center_nothing_left"><img src="$!webPath/resources/style/system/front/default/images/seller_center_nothing.png" /></div>
                <div class="seller_center_nothing_right">暂时没有数据</div>
              </div></td>
          </tr>
        </table>
        #end </div>
    </div>
  </div>
</div>
$!httpInclude.include("/footer.htm")
</body>
</html>
