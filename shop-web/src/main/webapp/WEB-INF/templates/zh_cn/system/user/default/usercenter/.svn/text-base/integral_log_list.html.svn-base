<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<title>积分兑换-$!{config.title}-$!{config.poweredby}</title>
<meta name="keywords" content="$!config.keywords" />
<meta name="description" content="$!config.description" />
<meta name="generator" content="$!{config.meta_generator}" />
<meta name="author" content="$!{config.meta_author}">
	<meta name="copyright" content="$!{config.copyRight}">
		<link
			href="$!webPath/resources/style/system/front/default/css/public.css"
			type="text/css" rel="stylesheet" />
		<link
			href="$!webPath/resources/style/system/front/default/css/public_auto.css"
			type="text/css" rel="stylesheet" />
		<link
			href="$!webPath/resources/style/system/front/default/css/user.css"
			type="text/css" rel="stylesheet" />
		<link href="$!webPath/resources/style/common/css/overlay.css"
			type="text/css" rel="stylesheet" />
		<link
			href="$!webPath/resources/style/common/css/jquery-ui-1.8.22.custom.css"
			type="text/css" rel="stylesheet" />
		<script src="$!webPath/resources/js/jquery-1.6.2.js"></script>
		<script src="$!webPath/resources/js/jquery-ui-1.8.21.js"></script>
		<script src="$!webPath/resources/js/jquery.poshytip.min.js"></script>
		<script src="$!webPath/resources/js/jquery.shop.common.js"></script>
		<script src="$!webPath/resources/js/jquery.validate.min.js"></script>
</head>
<body>
	$!httpInclude.include("/top.htm") $!httpInclude.include("/head.htm")
	$!httpInclude.include("/nav1.htm")
	<div class="main clearfix">
		<div class="usercenter">
			$!httpInclude.include("/buyer/nav.htm?op=integral_log")
			<div class="usercenter_right">
				<div class="myorder">
					<h4>
						<span class="myorder_fright"></span><strong>积分明细</strong>
					</h4>
				</div>
				<div class="user_list">
			        <div class="user_list_title">
			        
			          <ul class="user_title_ul">
			            <li class="this"><i></i><a href="$!webPath/buyer/integral_log_list.htm">积分明细</a></li>
			          </ul>
			        </div>
			        #if($!objs.size()>0)
			        <form method="post" id="ListForm">
			          <div class="show_smallimg">
			            <table width="100%" border="0" cellspacing="0" cellpadding="0" class="smallimg_table" >
			              <tr>
					       <!-- <th width="18%"><span class="form_btna">会员名</span></th> -->
					       <!-- <th width="18%" ><span class="form_btna">操作管理员</span></th> -->
					       <th width="10%" ><span class="form_btna">积分值</span></th>
					       <th width="15%" ><span class="form_btna">连续签到天数</span></th>
					       <th  width="15%"><span class="form_btna">操作时间</span></th>
					       <th width="15%"><span class="form_btna">获取途径</span></th>
					       <th width="15%"><span class="form_btna">余额</span></th>
					       <th ><span class="form_btna">描述</span></th>
					     </tr>
			              #foreach($obj in $objs)
			              	<tr>
						       <!-- <td align="center">$!obj.loginName</td> -->
						       <!-- <td align="center">$!obj.operateName</td> -->
						       <td align="center">$!obj.integral</td>
						       #if($!obj.type=="integral_sign_day")
						       <td align="center">$!obj.signDay</td>
						       #else
						       <td align="center">-</td>
						       #end
						       <td align="center">$!CommUtil.formatLongDate($!obj.addtime)</td>
						       #set($type=$!integralConstants.getNameByType($!obj.type))
							<td align="center">$!type</td>
							<td align="center">$!obj.balance</td>
						       <td align="center">$!obj.content</td>
						     </tr>
			              #end
			            </table>
			          </div>
			          <div class="userlist_allop">
			            <div class="user_list_page"><span class="user_page_sp">
			              <input name="currentPage" type="hidden" id="currentPage" value="$!currentPage" />
			              $!gotoPageFormHTML</span> </div>
			          </div>
			        </form>
			         #else
			          <div class="nothing">
			            <div class="nothing_left"><img src="$!webPath/resources/style/system/front/default/images/nothing.png" /></div>
			            <div class="nothing_right">您目前还没有积分明细记录！</div>
			          </div>
			          #end
			      </div>
			</div>
			
		</div>
	
	</div>

    $!httpInclude.include("/footer.htm")
</body>
</html>
